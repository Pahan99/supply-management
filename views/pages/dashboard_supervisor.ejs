<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Supervisor Dashboard | Supply Management</title>
    <link 
        href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" 
        rel="stylesheet" 
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous"
    >
    <link 
        rel="stylesheet" 
        href="https://use.fontawesome.com/releases/v5.15.3/css/all.css" 
        integrity="sha384-SZXxX4whJ79/gErwcOYf+zWLeJdY/qpuqC4cAa9rOGUstPomtqpuNWT9wdPEn2fk" 
        crossorigin="anonymous"
    >
</head>
<body>

    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container-fluid">
            <a class="navbar-brand text-uppercase" href="#">Supply Management System</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbar-content">
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item">
                        <a class="profile-name nav-link" aria-current="page" href="#">Sanka</a>
                    </li>
                    <li class="nav-item text-uppercase">
                        <a class="nav-link text-danger" aria-current="page" href="#">Logout</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <div class="container-fluid col-10">
        <div class="row mt-4 px-5 trip">
            <div class="my-3 px-3 d-grid accordion" id="trip">
                <div class="row accordion-item">
                    <div class="accordion-body col-md-7 col-10">
                        <h5 class="title route">Kandy - Colombo</h5>
                        <p class="body-text capacity">Capacity: 90/100kg</p>
                        <p class="body-text date">Date: 22/02/2022</p>
                        <p class="body-text journey-start">Journey Start: Kandy 10.00 a.m</p>
                        <p class="body-text journey-end">Journey End: Colombo 6.00 p.m</p>
                    </div>
                    <div class="buttons col-md-5 col-10 d-flex align-items-end justify-content-end">
                        <button type="button" class="btn btn-outline-dark m-2 collapsed" data-bs-toggle="collapse" data-bs-target="#products" aria-expanded="false" aria-controls="products">View Products</button>
                        <a href="#" class="btn btn-outline-danger m-2">Confirm</a>
                    </div>
                    <div id="products" class="accordion-collapse collapse" aria-labelledby="products-details" data-bs-parent="#trip">
                        <div class="accordion-body">
                            <table class="table table-striped table-hover products-details">
                                <thead>
                                    <tr>
                                        <th scope="col">Date</th>
                                        <th scope="col">Dispatch Location</th>
                                        <th scope="col">Related Store</th>
                                        <th scope="col">Order Date</th>
                                        <th scope="col">Weight</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>22/02/2022</td>
                                        <td>Temple Road, colombo</td>
                                        <td>colombo</td>
                                        <td>01/03/2022</td>
                                        <td>50kg</td>
                                    </tr>
                                    <tr>
                                        <td>22/02/2022</td>
                                        <td>Temple Road, colombo</td>
                                        <td>colombo</td>
                                        <td>01/03/2022</td>
                                        <td>50kg</td>
                                    </tr>
                                    <tr>
                                        <td>22/02/2022</td>
                                        <td>Temple Road, colombo</td>
                                        <td>colombo</td>
                                        <td>01/03/2022</td>
                                        <td>50kg</td>
                                    </tr>
                                    <tr>
                                        <td>22/02/2022</td>
                                        <td>Temple Road, colombo</td>
                                        <td>colombo</td>
                                        <td>01/03/2022</td>
                                        <td>50kg</td>
                                    </tr>
                                    <tr>
                                        <td>22/02/2022</td>
                                        <td>Temple Road, colombo</td>
                                        <td>colombo</td>
                                        <td>01/03/2022</td>
                                        <td>50kg</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
</body>
</html>